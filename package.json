    "@types/react-combine-reducers": "^1.0.0",
    "activity-detector": "^3.0.0",
    "axios": "^0.21.4",
    "cypress-social-logins": "^1.11.3",
    "date-fns": "^2.27.0",
    "deep-equal": "^2.0.5",
    "isomorphic-unfetch": "^3.1.0",
    "local-storage": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "namor": "^2.0.3",
    "nanoid": "^3.1.30",
    "next": "^12.0.4",
    "next-auth": "^3.25.0",
    "next-transpile-modules": "^9.0.0",
    "node-jose-tools": "^1.6.28",
    "react": "^17.0.2",
