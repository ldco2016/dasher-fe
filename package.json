{
  "homepage": "http://KnoxMed.github.io/dasher-fe",
  "name": "aluna-dashboard-next",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run lint",
    "dev": "react-env --path APP_ENV=local -- next dev",
    "build": "next build",
    "start": "next start  -p ${PORT:=3000}",
    "lint": "eslint --ext .ts,.tsx",
    "cy:open:chrome": "cypress open --browser chrome",
    "cy:run:chrome": "cypress run --headless",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:record": "cypress run --record --key 1a75ef6e-9d11-4a43-859d-59bfd949474f",
    "cy:run:VnV:firefox": "cypress run --browser firefox --spec 'cypress/integration/qms/**/*' --config screenshotsFolder=cypress/screenshots/firefox,trashAssetsBeforeRuns=false",
    "cy:run:VnV:chrome": "cypress run --browser chrome --spec 'cypress/integration/qms/**/*'  --config screenshotsFolder=cypress/screenshots/chrome",
    "cy:run:VnV:all": "npm run cy:run:VnV:chrome && npm run cy:run:VnV:firefox",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "dependencies": {
    "@beam-australia/react-env": "^3.1.1",
